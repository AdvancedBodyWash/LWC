#include <stdio.h>

int main()
{
	int num;                  //조건식에 대입할 값, 몇 번 반복할지 수를 정함
	int i = 1;                //초기값 
	
	printf("숫자를 입력: ");    
	scanf_s("%d",&num);        //num에 값 대입

	while (num >= i) {      //조건식 

		int result;                         //반복문 안의 식
		result = 3 * i;

		printf("3의 배수값:%d \n",result);

		i++;                  //증감식
	}
}
