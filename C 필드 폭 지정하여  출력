#include <stdio.h>

int main(void)
{
	printf("%-8s %14s %5s \n", "이  름", "전공학과", "학년"); //첫번째는 8칸확보 왼쪽으로 출력, 두번째는 14칸확보 오른쪽 출력 3번째는 5칸확보 오른쪽 출력
	printf("%-8s %14s %5d \n", "김동수", "전자공학", 3);
	printf("%-8s %14s %5d \n", "이을수", "컴퓨터공학", 2);
	printf("%-8s %14s %5d \n", "한선영", "미술교육학", 4);
	return 0;
}
